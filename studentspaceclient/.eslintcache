hboard route */}\r\n          <PrivateRoute exact path =\"/dashboard\" component={Dashboard}/>\r\n\r\n          <PrivateRoute exact path =\"/create-profile\" component={ProfileForm}/>\r\n\r\n          <PrivateRoute exact path =\"/profile\" component={Profile}/>\r\n\r\n          <PrivateRoute exact path =\"/community\" component={Community}/>\r\n\r\n          <PrivateRoute exact path ='/community/:id' component={Post}/>\r\n\r\n          <PrivateRoute exact path ='/events' component={Events}/>\r\n        \r\n          <Route component={NotFound}/>\r\n        \r\n        </Switch>\r\n      </Fragment>\r\n    </Router>\r\n\r\n    </Provider>\r\n\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Amol Telang\\Documents\\MERN projects\\minorProject\\minorProject\\studentspace\\studentspaceclient\\src\\store.js",[],"C:\\Users\\Amol Telang\\Documents\\MERN projects\\minorProject\\minorProject\\studentspace\\studentspaceclient\\src\\utils\\setAuthToken.js",[],"C:\\Users\\Amol Telang\\Documents\\MERN projects\\minorProject\\minorProject\\studentspace\\studentspaceclient\\src\\actions\\auth.js",[],"C:\\Users\\Amol Telang\\Documents\\MERN projects\\minorProject\\minorProject\\studentspace\\studentspaceclient\\src\\components\\layout\\Landing.js",[],"C:\\Users\\Amol Telang\\Documents\\MERN projects\\minorProject\\minorProject\\studentspace\\studentspaceclient\\src\\components\\auth\\Register.js",["167","168","169","170"],"import React,{useState} from 'react';\r\nimport {Link,Redirect} from 'react-router-dom';\r\nimport {connect} from 'react-redux';\r\nimport PropTypes from 'prop-types'\r\n\r\n//importing actions\r\nimport {setAlert} from '../../actions/alert';\r\nimport {userRegister} from '../../actions/auth';\r\n\r\n//importing logo\r\nimport avatar from '../../images/registerIcons/avatar.png';\r\nimport blackVector from '../../images/registerIcons/blackvector.png'\r\nimport logo from '../../images/registerIcons/logo.png';\r\n\r\n\r\n\r\n\r\n\r\nconst Register=({setAlert