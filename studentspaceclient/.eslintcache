_ALERT\n} from '../actions/types'\n\n//initial state\nconst initialState=[];\n\n\n\n\nexport default function(state=initialState,action)\n{   \n    //destructuring data from the action\n    const{type,payload}=action\n    switch(type)\n    {\n        //sets an alert\n        case SET_ALERT:\n            return[...state,payload]\n\n        //removes an alert    \n        case REMOVE_ALERT:\n            return state.filter(alert=>alert.id!==payload)\n            \n        //return a state by default\n        default:\n            return state;\n    }\n}","/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/actions/types.js",[],"/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/components/layout/Alert.js",[],"/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/actions/alert.js",[],"/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/actions/auth.js",[],"/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/reducers/auth.js",["161","162"],"/home/amol/MERN Projects/student space/studentspace/studentspaceclient/src/utils/setAuthToken.js",[],"/home/amol/Minr Project/studentspace/studentspaceclient/src/index.js",[],["163","164"],"/home/amol/Minr Project/studentspace/studentspaceclient/src/App.js",["165"],"/home/amol/Minr Project/studentspace/studentspaceclient/src/reportWebVitals.js",[],"/home/amol/Minr Project/studentspace/studentspaceclient/src/store.js",[],"/home/amol/Minr Project/studentspace/studentspaceclient/src/utils/setAuthToken.js",[],"/home/amol/Minr Project/studentspace/studentspaceclient/src/actions/auth.js",[],"/home/amol/Minr Project/studentspace/studentspaceclient/src/components/layout/Landing.js",["166"],"/home/amol/Minr Project/studentspace/studentspaceclient/src/components/auth/Register.js",["167","168","169"],"import React,{useState} from 'react';\nimport {Link,Redirect} from 'react-router-dom';\nimport {connect} from 'react-redux';\nimport 