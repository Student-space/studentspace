ading:true,\n    user:null\n}\n\nexport default function(state=initalState,action)\n{\n    const{type,payload}=action;\n    switch(type)\n    {\n      case REGISTER_SUCCESS:\n          return{\n            ...state,\n            isAuthenticated:true,\n            loading:false\n          }\n          \n       case LOGIN_SUCCESS:\n         \n          localStorage.setItem('token',payload.refreshTokens);\n          return{\n              ...state,\n              payload,\n              isAuthenticated:true,\n              loading:false\n\n          }\n\n          case REGISTER_FAIL:\n            case AUTH_ERROR:\n            case LOGIN_FAIL:\n            case LOGOUT:\n              localStorage.removeItem('token');\n            return{\n                ...state,\n                token:null,\n                isAuthenticated:false,\n                loading:false\n            }\n            \n           case USER_LOADED:\n               return{\n                ...state,\n                isAuthenticated:true,\n                loading:false,\n                user:payload\n\n               }\n            \n               \n          default:\n          return state;\n    }\n}","/home/amol/Minr Project/studentspace/studentspaceclient/src/components/library/Library.js",["184"],{"ruleId":"185","replacedBy":"186"},{"ruleId":"187","replacedBy":"188"},{"ruleId":"189","severity":1,"message":"190","line":3,"column":35,"nodeType":"191","messageId":"192","endLine":3,"endColumn":43},{"ruleId":"189","severity":1,"message":"193","line":2,"column":8,"nodeType":"191","messageId":"192","endLine":2,"endColumn":17},{"ruleId":"189","severity":1,"message":"194","line":1,"column":17,"nodeType":"191","messageId":"192","endLine":1,"endColumn":25},{"ruleId":"189","severity":1,"message":"195","line":32,"column":10,"nodeType":"191","messageId":"192","endLine":32,"endColumn":18},{"ruleId":"196","severity":1,"message":"197","line":48,"column":9,"nodeType":"198","endLine":50,"endColumn":61},{"ruleId":